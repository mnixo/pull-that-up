{
  "entrypoint": "index.html",
  "npm": true,
  "moduleResolution": "node",
  "sources": [
    "src/app/ptu-app.js"
  ],
  "extraDependencies": [
    "node_modules/@webcomponents/webcomponentsjs/**"
  ],
  "builds": [
    {
      "name": "es6-bundled",
      "bundle": true,
      "browserCapabilities": [
        "es2015"
      ],
      "js": {
        "minify": true,
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      }
    }
  ]
}
